{
    "manifest_version": 3,
    "name": "ImageSwitcher",
    "version": "1.0.1",
    "icons": {
        "16": "icons/icon16.png",
        "32": "icons/icon32.png",
        "48": "icons/icon48.png"
    },
    "background": {
        "service_worker": "background.js",
        "type":"module"
    },
    "action": {
        "default_popup": "index.html"
    },
    "permissions":[
        "scripting"
    ],
    "host_permissions": [
        "http://*/*",
        "https://*/*"
    ]

}